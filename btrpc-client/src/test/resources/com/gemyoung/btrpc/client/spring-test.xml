<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="remoteService" class="com.gemyoung.btrpc.client.RpcReqProxyFactoryBean">
        <property name="rpcInterface" value="com.gemyoung.btrpc.client.RemoteService" />
    </bean>
    <bean id="remoteService2" class="com.gemyoung.btrpc.client.RpcReqProxyFactoryBean">
        <property name="rpcInterface" value="com.gemyoung.btrpc.client.RemoteService2" />
    </bean>
</beans>